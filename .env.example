API_KEY="your-api-key"
API_ENDPOINT="https://api.siliconflow.cn/v1/chat/completions"
API_MODEL="Qwen/Qwen3-8B"
PORT=3000
# 你的邮件服务商的 SMTP 服务器地址
EMAIL_HOST=smtp.163.com

# SMTP 端口 (例如，对于大部分邮箱SSL是 465)
EMAIL_PORT=465

# 是否使用 SSL/TLS (对于 465 端口应为 true)
EMAIL_SECURE=true

# 你的发件人邮箱地址
EMAIL_USER=

# 你的邮箱授权码 (注意：不是邮箱登录密码！)
# QQ邮箱等需要开启SMTP服务后获取一个专用的“授权码”
EMAIL_PASS=

# 你希望接收客户咨询的邮箱地址
EMAIL_TO=